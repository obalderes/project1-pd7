sox = []
sivin = []

purple = open( "ml7-student-names" );
orange = purple.readlines();


for line in orange:
	line = line.strip();
	if line[-1] == 6:
		sox.append(line);
	elif line[-1] == 7:
		sivin.append(line);
		
print orange;
print sox
print sivin
	
	
