period6 = list()
period7 = list()
ml7 = open("ml7-student-names") 

for lines in ml7.readlines():
    #print lines 
    lines = lines.strip()
    l, f, p = lines.split(",") #I keep on getting a value error
    if (p == "6"):
        period6.append(lines)
    if (p == "7"):
        period7.append(lines)

print period6
print period7
        
    
